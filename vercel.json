{
  "version": 2,
  "builds": [
    { "src": "BackEnd/index.js", "use": "@vercel/node" },
    { "src": "BackEnd/dist/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "BackEnd/index.js" },
    { "src": "/assets/(.*)", "dest": "/BackEnd/dist/assets/$1" },
    { "src": "/(.*)", "dest": "/BackEnd/dist/index.html" }
  ]
}
